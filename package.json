{
  "name": "@u4/opencv-build",
  "version": "0.3.1",
  "description": "A simple script to auto build recent OpenCV + contrib version via npm 2021 Edition",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "publishConfig": {"access": "public"},
  "scripts": {
    "clean": "rimraf build",
    "build": "tsc",
    "build3x": "tsc && node ./build/main.js --keepsource",
    "do-install": "node ./build/main.js",
    "cleanbuild": "rimraf build && tsc",
    "postinstallDISABLED": "npm run do-install",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "test": "mocha --recursive ./test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UrielCh/npm-opencv-build.git"
  },
  "keywords": [
    "opencv",
    "build",
    "opencv4nodejs"
  ],
  "contributors": [
    "Uriel Chemouni <uchemouni@gmail.com> (https://urielch.github.io/)",
    "justadudewhohacks (https://github.com/justadudewhohacks)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UrielCh/npm-opencv-build/issues"
  },
  "homepage": "https://github.com/UrielCh/npm-opencv-build#readme",
  "dependencies": {
    "npmlog": "^6.0.0",
    "picocolors": "^1.0.0",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.2",
    "@types/npmlog": "^4.1.3",
    "@types/rimraf": "^3.0.2",
    "chai": "^4.2.0",
    "mocha": "^6.2.0",
    "typescript": "^4.5.4"
  },
  "files": [
    "build",
    "Find-VS2017.cs"
  ]
}
